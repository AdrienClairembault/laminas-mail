{
    "name": "laminas/laminas-mail",
    "description": "provides generalized functionality to compose and send both text and MIME-compliant multipart e-mail messages",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "mail"
    ],
    "homepage": "https://laminas.dev",
    "support": {
        "docs": "https://docs.laminas.dev/laminas-mail/",
        "issues": "https://github.com/laminas/laminas-mail/issues",
        "source": "https://github.com/laminas/laminas-mail",
        "rss": "https://github.com/laminas/laminas-mail/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.4-dev",
            "dev-develop": "2.5-dev"
        }
    },
    "require": {
        "php": ">=5.3.23",
        "laminas/laminas-crypt": "~2.4.0",
        "laminas/laminas-loader": "~2.4.0",
        "laminas/laminas-mime": "~2.4.0",
        "laminas/laminas-stdlib": "~2.4.0",
        "laminas/laminas-validator": "~2.4.0",
        "laminas/laminas-zendframework-bridge": "^1.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "1.7.*",
        "laminas/laminas-config": "~2.4.0",
        "laminas/laminas-servicemanager": "~2.4.0",
        "phpunit/phpunit": "~4.0",
        "satooshi/php-coveralls": "dev-master"
    },
    "suggest": {
        "laminas/laminas-servicemanager": "Laminas\\ServiceManager component"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\Mail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasTest\\Mail\\": "test/"
        }
    },
    "replace": {
        "zendframework/zend-mail": "self.version"
    }
}
